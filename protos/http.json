{
  "server": [
    {
      "read_until": "\r\n",
      "complie_regex": "(?<action>\\w+)\\s{1}(?<url>\\S+)\\s{1}(?<version>\\S+)",
      "next_condition": {}
    },
    {
      "read_regex": "(?<key_0>.*):{0,1}(?<value_0>.*)\r\n",
      "complie_regex": "",
      "next_condition": { "match": "^\r\n$" }
    },
    {
      "read_size": "$Content-Length",
      "complie_regex": "",
      "next_condition": {}
    }
  ],
  "client": [
    {
      "read_until": "\r\n",
      "complie_regex": "(?<version>\\w+)\\s(?<status_code>\\S+)\\s(?<status>\\S+)",
      "next_condition": {}
    },
    {
      "read_until": "\r\n",
      "complie_regex": "(?<key_0>.*):{0,1}(?<value_0>.*)",
      "next_condition": { "match": "^\r\n$" }
    },
    {
      "read_size": "$Content-Length",
      "complie_regex": "",
      "next_condition": {}
    }
  ]
}
